{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "test": "jest --watch --coverage=false --changedSince=master",
    "test-ci": "jest --coverage=true",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint src --max-warnings=0 --ext .js,.jsx",
    "lint-fix": "eslint src --fix --ext .js,.jsx",
    "prettier": "prettier 'src/**/*.{ts,tsx,js,jsx}' --list-different",
    "prettier-fix": "prettier 'src/**/*.{ts,tsx,js,jsx}' --write"
  },
  "dependencies": {
    "@react-native-community/masked-view": "0.1.6",
    "@react-navigation/material-bottom-tabs": "^5.2.9",
    "@react-navigation/native": "^5.1.1",
    "@react-navigation/routers": "^5.3.0",
    "@react-navigation/stack": "^5.2.9",
    "expo": "^37.0.7",
    "expo-av": "~8.1.0",
    "expo-localization": "~8.1.0",
    "i18n-js": "^3.5.1",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-37.0.0.tar.gz",
    "react-native-animatable": "^1.3.3",
    "react-native-collapsible": "^1.5.2",
    "react-native-elements": "^1.2.7",
    "react-native-gesture-handler": "~1.6.0",
    "react-native-kill-packager": "^1.0.0",
    "react-native-paper": "^3.10.1",
    "react-native-reanimated": "~1.7.0",
    "react-native-safe-area-context": "0.7.3",
    "react-native-screens": "~2.2.0",
    "react-native-snap-carousel": "^3.9.0",
    "react-native-svg": "^12.1.0",
    "react-native-swipe-gestures": "^1.0.5",
    "react-native-web": "^0.11.7",
    "react-native-webview": "8.1.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.10.0",
    "watchman": "^1.0.0",
    "expo-screen-orientation": "~1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "babel-preset-expo": "^8.1.0",
    "eslint": "^6.8.0",
    "eslint-config-universe": "^3.0.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "eslint-plugin-react-native": "^3.8.1",
    "husky": "^4.2.3",
    "jest-expo": "^37.0.0",
    "lint-staged": "^10.1.2",
    "nock": "^12.0.3",
    "node-fetch": "^2.6.0",
    "prettier": "^1.19.1",
    "react-native-testing-library": "^1.13.0",
    "react-test-renderer": "^16.13.1",
    "redux-mock-store": "^1.5.4",
    "typescript": "^3.8.3"
  },
  "lint-staged": {
    "**/*.{js,jsx,css}": [
      "npm run lint-fix",
      "npm run prettier-fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base)"
    ],
    "setupFiles": [
      "./setupJest.js"
    ]
  },
  "engines": {
    "node": ">=0.12"
  },
  "private": true,
  "license": "MIT"
}
